pipeline{
    agent any
    stages{
        stage("checkout"){
            steps{
                sh "git clone https://github.com/shubh1sinha/order-management.git"
            }
        }
        
         stage("package"){
            steps{
            sh "mvn clean package"
            }
        }
		stage('Docker Run') {

			steps {
				sh 'docker-compose up'
			}
		}
	}
    }
  